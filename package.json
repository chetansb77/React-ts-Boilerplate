{
  "name": "react-ts-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "compose-up-sonarqube": "docker-compose -f docker-compose-sonarqube.yml up -d",
    "compose-down-sonarqube": "docker-compose -f docker-compose-sonarqube.yml down",
    "stop-sonarqube": "docker-compose -f docker-compose-sonarqube.yml stop",
    "sonarqube-scan": "node ./sonarqube/sonarscan.cjs"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/addon-styling": "^1.0.1",
    "@storybook/blocks": "^7.0.5",
    "@storybook/react": "^7.0.5",
    "@storybook/react-vite": "^7.0.5",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.22",
    "postcss-loader": "^7.2.4",
    "prop-types": "^15.8.1",
    "sass": "^1.62.0",
    "sonarqube-scanner": "^3.0.1",
    "storybook": "^7.0.5",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.0",
    "vitest": "^0.30.1"
  }
}
